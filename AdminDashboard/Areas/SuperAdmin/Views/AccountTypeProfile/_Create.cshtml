
<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-body">
                    <input type="hidden" id="accountTypeProfileId" />
                    <div class="form-group">
                        <label class="control-label">AccoutType <span class="validation-error">*</span></label>
                        <select id="accountTypeID" asp-items="@ViewBag.accountTypes"  class="form-control enableBtn" data-style="select-with-transition" title="Choose a Accout Type " data-size="7">
                            <option value="0" disabled selected> Choose a AccountType</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label class="control-label">Profiles <span class="validation-error">*</span></label>
                        <select id="profileID" asp-items="@ViewBag.profiles"  class="form-control enableBtn" data-style="select-with-transition" title="Choose a Profile" data-size="7">
                            <option value="0" disabled selected> Choose a Profile</option>
                        </select>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

